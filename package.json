{
  "name": "verilens",
  "displayName": "VeriLens",
  "description": "",
  "version": "0.0.4",
  "publisher": "furinara",
  "icon": "icon/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/Furinara/verilens"
  },
  "engines": {
    "vscode": "^1.104.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "languages": [
    {
      "id": "systemverilog",
      "aliases": [],
      "extensions": [
        ".sv",
        ".svh"
      ],
      "configuration": "config/systemverilog.json"
    },
    {
      "id": "verilog",
      "aliases": [],
      "extensions": [
        ".v",
        ".vh"
      ],
      "configuration": "config/verilog.json"
    }
  ],
    "grammars": [{
        "language": "systemverilog",
        "scopeName": "source.sv",
        "path": "./syntaxes/systemverilog.tmLanguage.json"
      },{
        "language": "verilog",
        "scopeName": "source.v",
        "path": "./syntaxes/verilog.tmLanguage.json"
      }
    ],
    "snippets": [{
        "language": "systemverilog",
        "path": "./snippets/systemverilog-snippets.json"
      },{
        "language": "verilog",
        "path": "./snippets/verilog-snippets.json"
      }
    ]
  }
}
